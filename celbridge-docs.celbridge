[celbridge]
celbridge-version = "0.2.3"

[project]
requires-python = "3.12"
dependencies = [
    "sphinx",
    "sphinx_rtd_theme",
    "sphinx_new_tab_link",
    "pygments==2.19.2",
    "sphinx-copybutton",
    "myst-parser",
    "myst-parser[linkify]"
]

[[shortcut]]
name = "make"
script = '''
run "make.ipy"
'''

[[shortcut]]
name = "serve"
script = '''
run "serve.ipy"
'''
